def even_odd_number(number,base):
	even = []
	while number >= base:
		val = number % base
		if val == 0:
			even.append(number)
		number -= 1
	return even

def prime_number(number):
	temp = 0
	count = 2
	val = number // 2
	while count <= 	val:
		temp = number % count
		if temp == 0:
			return 0
		else:
			count += 1
	if temp  != 0:
		return number

def factors(number):
	count = 1
	sum = 0 
	factors = []
	while count <= number:
		temp = number % count
		if temp == 0:
			factors.append(count)
			sum += count
		count += 1

	return factors, count

